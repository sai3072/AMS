        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	
	//Get AMS resource object
	var create = new AMS_CaseFetch();
	var ccemails;
	//Check empty value for cc_emails
	if(current.u_case_cc_emails == '')
		ccemails = '';
	else
		ccemails = [current.u_case_cc_emails.toString()];
	//API call to create case
	var caseId = create.createCaseAndInsert(current.u_case_category.toString(),ccemails,current.u_case_details.toString(),current.u_case_type.toString(),current.u_case_service_code.toString(),'low',current.u_case_subject.toString());
	
	if(caseId != false)
		{
		current.u_case_id = caseId;
		gs.addInfoMessage('Service Request successfully created');
		current.update();
	}
	else
		{
		current.setAbortAction(true);
		gs.addInfoMessage('Service Request creation Failed');
	}
})(current, previous);
]]></script>
